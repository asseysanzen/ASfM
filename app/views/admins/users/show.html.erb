<div class="headline">
	<h3 class="headline_text"><%= @user.name %></h3>
</div>

<div class="customer_content">
	<div class="admin_c_label">
		<p>ID</p>
	</div>
	<div class="explain_text">
		<%= @user.id %>
		<div class="c_status">
			<%= @user.status %>
		</div>
	</div>
</div>

<div>
	<%= render 'partials/user', user: @user %>
</div>

<div class="c_button">
	<%= link_to "編集", edit_admins_user_path(@user.id), class:"btn btn-primary" %>
</div>

<div>
    <%= render 'partials/posts', posts: @posts %>
</div>